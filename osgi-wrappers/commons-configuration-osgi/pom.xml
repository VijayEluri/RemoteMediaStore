  <groupId>commons-configuration</groupId>
  <artifactId>commons-configuration-osgi</artifactId>
  <version>1.8</version>
  <packaging>bundle</packaging>
  <name>OSGi wrapper for Commons Configuration</name>

  <properties>
    <bundle.artifactId>commons-configuration</bundle.artifactId>
    <osgi.import-package>
      javax.servlet; resolution:="optional",
      org.apache.xml.resolver.*; resolution:="optional",
      org.apache.commons.jexl2; resolution:="optional",
